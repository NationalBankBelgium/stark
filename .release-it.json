{
	"non-interactive": false,
	"dry-run": false,
	"verbose": false,
	"force": false,
	"pkgFiles": ["package.json", "package-lock.json"],
	"increment": "conventional:angular",
	"scripts": {
		"afterRelease": false,
		"beforeStage": "npm run check:nightly:stark-versions -- ${version} && npm run generate:changelog",
		"beforeStart": "npm run check:starter:stark-versions"
	},
	"git": {
		"changelog": "npm run generate:changelog-recent",
		"requireCleanWorkingDir": true,
		"requireUpstream": true,
		"commit": true,
		"commitMessage": "chore(release): release %s",
		"commitArgs": "",
		"tag": true,
		"tagName": "%s",
		"tagAnnotation": "%s",
		"push": true,
		"pushArgs": "",
		"pushRepo": "origin"
	},
	"npm": {
		"publish": false
	},
	"github": {
		"release": true,
		"releaseName": "Release %s",
		"draft": false,
		"tokenRef": "GITHUB_TOKEN",
		"assets": null,
		"host": null
	}
}
