{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "lib": {
    "entryFile": "public_api.ts",
    "flatModuleFile": "stark-ui"
  },
  "dest": "../../dist/packages/stark-ui",
  "deleteDestPath": true,
  "assets": [
    "ngcc.config.js",
    "./assets",
    "**/assets/**/*.json",
    "**/assets/**/*.d.ts",
    "./styles",
    "_index.scss",
    "src/**/*.scss",
    "src/**/*.md",
    "testing/**/*.md"
  ],
  "allowedNonPeerDependencies": [
    "@angular/material-moment-adapter",
    "@mdi/angular-material",
    "@prettier/plugin-xml",
    "@sqltools/formatter",
    "@types/lodash-es",
    "@types/nouislider",
    "@types/prismjs",
    "angular2-text-mask",
    "normalize.css",
    "nouislider",
    "prettier",
    "pretty-data",
    "prismjs",
    "text-mask-addons",
    "text-mask-core"
  ]
}
