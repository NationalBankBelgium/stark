{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "lib": {
    "entryFile": "public_api.ts",
    "flatModuleFile": "stark-core"
  },
  "dest": "../../dist/packages/stark-core",
  "deleteDestPath": true,
  "assets": ["./assets", "testing/**/*.md", "ngcc.config.js"],
  "allowedNonPeerDependencies": [
    "@angularclass/hmr",
    "@ng-idle/core",
    "@ng-idle/keepalive",
    "@ngrx/effects",
    "@ngrx/store",
    "@ngrx/store-devtools",
    "@ngx-translate/core",
    "@types/lodash-es",
    "@types/uuid",
    "@uirouter/angular",
    "@uirouter/core",
    "@uirouter/rx",
    "cerialize",
    "class-validator",
    "deep-freeze-strict",
    "ibantools",
    "lodash-es",
    "moment",
    "uuid"
  ]
}
