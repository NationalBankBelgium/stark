{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "lib": {
    "entryFile": "public_api.ts",
    "umdModuleIds": {
      "@angularclass/hmr": "angularclass.hmr",
      "@ng-idle/core": "ngIdle.core",
      "@ng-idle/keepalive": "ngIdle.keepalive",
      "@ngrx/effects": "ngrx.effects",
      "@ngrx/store": "ngrx.store",
      "@ngx-translate/core": "ngxTranslate.core",
      "@uirouter/angular": "uirouter.angular",
      "@uirouter/core": "uirouter.core",
      "@uirouter/rx": "uirouter.rx",
      "cerialize": "cerialize",
      "class-validator": "class-validator",
      "ibantools": "ibantools",
      "lodash-es/cloneDeep": "lodash-es.cloneDeep",
      "lodash-es/floor": "lodash-es.floor",
      "lodash-es/isEmpty": "lodash-es.isEmpty",
      "lodash-es/noop": "lodash-es.noop",
      "lodash-es/reduce": "lodash-es.reduce",
      "moment": "moment",
      "uuid": "uuid",
      "zone.js/dist/long-stack-trace-zone": "zoneJs.dist.longStackTraceZone"
    },
    "flatModuleFile": "stark-core"
  },
  "dest": "../../dist/packages/stark-core",
  "deleteDestPath": true,
  "assets": ["./assets", "testing/**/*.md"],
  "allowedNonPeerDependencies": [
    "@angularclass/hmr",
    "@ng-idle/core",
    "@ng-idle/keepalive",
    "@ngrx/effects",
    "@ngrx/store",
    "@ngrx/store-devtools",
    "@ngx-translate/core",
    "@types/lodash-es",
    "@types/uuid",
    "@uirouter/angular",
    "@uirouter/core",
    "@uirouter/rx",
    "cerialize",
    "class-validator",
    "deep-freeze-strict",
    "ibantools",
    "lodash-es",
    "moment",
    "uuid"
  ]
}
