{
  "name": "stark-showcase",
  "version": "0.0.1",
  "private": true,
  "description": "Stark - Showcase",
  "keywords": [
    "angular",
    "angular6",
    "webpack",
    "typescript",
    "redux",
    "ngrx",
    "ui-router"
  ],
  "author": "Stark Team <alexis.georges@nbb.be>",
  "contributors": [],
  "license": "MIT",
  "bugs": "https://github.com/nationalbankbelgium/stark/issues",
  "homepage": "https://github.com/nationalbankbelgium/stark",
  "repository": {
    "type": "git",
    "url": "https://github.com/nationalbankbelgium/stark.git"
  },
  "engines": {
    "node": ">=18.17.1",
    "npm": ">=10.1.0"
  },
  "scripts": {
    "build": "npm run build:dev",
    "build:aot": "npm run build:aot:prod",
    "build:aot:dev": "npm run build:dev -- --aot",
    "build:aot:prod": "npm run clean:dist && npm run clean:aot && npm run ng -- build -c production",
    "build:dev": "npm run clean:dist && npm run ng -- build",
    "build:dev:bundle-analyzer": "npx mkdirp reports && cross-env BUNDLE_ANALYZER=1 npm run build:dev",
    "build:prod": "npm run clean:dist && npm run build:aot:prod",
    "build:prod:bundle-analyzer": "npm run clean:dist && cross-env BUNDLE_ANALYZER=1 npm run build:aot:prod",
    "build:prod:ghpages": "npm run clean:dist && node ./ghpages-adapt-angular-json.js && npm run build:aot:prod",
    "check-deps": "npx npm-check-u",
    "ci": "npm run ci:testall",
    "ci:aot": "cross-env BUILD_E2E=1 npm run lint && npm run test && npm run build:prod && npm run e2e",
    "ci:nobuild": "npm run lint && npm test && npm run e2e",
    "ci:testall": "cross-env BUILD_E2E=1 npm run lint && npm run test && npm run build:prod && npm run e2e",
    "clean": "npm cache clean --force && npm run rimraf -- node_modules doc coverage reports dist compiled dll",
    "clean:aot": "npm run rimraf -- compiled",
    "clean:dist": "npm run rimraf -- dist",
    "clean:dll": "npm run rimraf -- dll",
    "clean:install": "npm set progress=false && npm install",
    "clean:modules": "npm run rimraf -- node_modules package-lock.json",
    "e2e": "npm-run-all -p -r server:prod:ci protractor",
    "e2e:browserstack": "npm-run-all -p -r server:prod:ci protractor:browserstack",
    "e2e:live": "npm-run-all -p -r server:prod:ci protractor:live",
    "github-deploy": "npm run github-deploy:dev",
    "github-deploy:dev": "npm run webpack -- --config node_modules/@nationalbankbelgium/stark-build/config/webpack.github-deploy.js --progress --profile --env.githubDev",
    "github-deploy:prod": "npm run webpack -- --config node_modules/@nationalbankbelgium/stark-build/config/webpack.github-deploy.js --progress --profile --env.githubProd",
    "karma": "karma",
    "lint": "npm run lint-es && npm run lint-css",
    "lint-css": "stylelint \"./src/**/*.?(pc|sc|c|sa)ss\" --formatter \"string\"",
    "lint-es": "npm run ng -- lint",
    "lint-es:fix": "npm run lint-es -- --fix",
    "ng": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng",
    "ngc": "ngc",
    "node": "node",
    "postversion": "git push && git push --tags",
    "preclean:install": "npm run clean",
    "prettier-check": "prettier **/*.{css,js,json,md,pcss,scss,ts} --write",
    "preversion": "npm test",
    "protractor": "protractor ./e2e/protractor.conf",
    "protractor:browserstack": "protractor ./e2e/protractor.browserstack.conf.js",
    "protractor:delay": "sleep 3 && npm run protractor",
    "protractor:live": "protractor --elementExplorer",
    "rimraf": "npx rimraf",
    "server": "npm run server:dev",
    "server:aot:dev": "npm run server:dev -- --aot",
    "server:dev": "npm run clean:dist && npm run ng -- serve",
    "server:dev:bundle-analyzer": "npm run clean:dist && cross-env BUNDLE_ANALYZER=1 npm run ng -- serve",
    "server:dev:hmr": "npm run server:dev -- --configuration hmr",
    "server:prod": "http-server dist -c-1 --cors",
    "server:prod:ci": "http-server dist -p 3000 -c-1 --cors",
    "start": "npm run server:dev",
    "start:aot": "npm run server:aot:dev",
    "start:bundle-analyzer": "npm run server:dev:bundle-analyzer",
    "start:hmr": "npm run server:dev:hmr",
    "test": "npm run lint && npm run test-fast",
    "test-fast": "npm run ng test",
    "test-fast:ci": "npm run ng -- test --watch=false --code-coverage",
    "test:ci": "npm run lint && npm run test-fast:ci",
    "tsc": "tsc",
    "version": "npm run build",
    "watch": "npm run watch:dev",
    "watch:aot:dev": "npm run build:aot:dev -- --watch",
    "watch:aot:prod": "npm run build:aot:prod -- --watch",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "webdriver-manager": "node ./node_modules/protractor/bin/webdriver-manager",
    "webdriver:start": "node ./node_modules/protractor/bin/webdriver-manager start",
    "webdriver:update": "node ./node_modules/protractor/bin/webdriver-manager update",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js"
  },
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "^16.2.14",
    "@angular/cli": "^16.2.16",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/compiler-cli": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^16.2.12",
    "@angular/material": "^16.2.14",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/platform-server": "^16.2.12",
    "@angular/router": "^16.2.12",
    "@nationalbankbelgium/code-style": "^1.11.0",
    "@nationalbankbelgium/ngx-form-errors": "2.0.1",
    "@nationalbankbelgium/stark-core": "file:../dist/packages-dist/stark-core/nationalbankbelgium-stark-core-12.0.0-rc.0-f7a24d7d-1728638970.tgz",
    "@nationalbankbelgium/stark-rbac": "file:../dist/packages-dist/stark-rbac/nationalbankbelgium-stark-rbac-12.0.0-rc.0-f7a24d7d-1728638970.tgz",
    "@nationalbankbelgium/stark-ui": "file:../dist/packages-dist/stark-ui/nationalbankbelgium-stark-ui-12.0.0-rc.0-f7a24d7d-1728638970.tgz",
    "@uirouter/visualizer": "~7.2.1",
    "angular-in-memory-web-api": "~0.16.0",
    "basscss": "~8.1.0",
    "core-js": "~3.17.2",
    "eligrey-classlist-js-polyfill": "~1.2.20180112",
    "hammerjs": "^2.0.8",
    "http-server": "~14.1.0",
    "ngrx-store-logger": "~0.2.2",
    "reflect-metadata": "~0.2.2",
    "rxjs": "^7.8.1",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@compodoc/compodoc": "1.1.26",
    "@nationalbankbelgium/eslint-config": "^16.0.0",
    "@nationalbankbelgium/stark-build": "file:../dist/packages-dist/stark-build/nationalbankbelgium-stark-build-12.0.0-rc.0-f7a24d7d-1728638970.tgz",
    "@nationalbankbelgium/stark-testing": "file:../dist/packages-dist/stark-testing/nationalbankbelgium-stark-testing-12.0.0-rc.0-f7a24d7d-1728638970.tgz",
    "@types/core-js": "~2.5.4",
    "@types/hammerjs": "~2.0.39",
    "@types/node": "^18.19.4",
    "@types/webpack-env": "~1.18.5",
    "browserstack-local": "~1.5.1",
    "cross-env": "~7.0.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "find-root": "~1.1.0",
    "npm-run-all": "~4.1.5",
    "parse5": "~7.1.2",
    "ts-node": "~10.9.2",
    "typescript": "~4.9.5"
  }
}
